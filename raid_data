
const insert = db.prepare('INSERT INTO raid_data (raid_id, raid_name, level_unlocked, image_url, host_material, wiki) VALUES (@raid_id, @raid_name, @level_unlocked, @image_url, @host_material, @wiki)');
const insertMany = db.transaction ((raids) => {
    for(const raid of raids) insert.run(raid);
});

